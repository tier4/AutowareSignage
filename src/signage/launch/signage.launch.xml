  <arg name="signage_stand_alone" default="false" />
  <arg name="ignore_emergency_stoppped" default="false" />
  <node pkg="signage" exec="signage"  output="screen">
    <param name="signage_stand_alone" value="$(var signage_stand_alone)" />
