<?xml version="1.0"?>
<launch>
  <arg name="signage_param" default="$(find-pkg-share signage)/config/signage_param.yaml" />
  <node pkg="signage" exec="signage"  output="screen">
    <param from="$(var signage_param)"/>
  </node>

  <include file="$(find-pkg-share boot_shutdown_interface)/launch/boot_shutdown.launch.xml">
    <arg name="ecu_name" value="signage0" />
  </include>
</launch>
